{
  "name": "moonshot-ai-workspace",
  "version": "1.0.0",
  "description": "Moonshot AI workspace root package configuration",
  "private": true,
  "workspaces": [
    "vscode_extension",
    "vscode_extension/shared",
    "agent_sdk"
  ],
  "scripts": {
    "lint": "eslint ./**/*.ts ./**/*.tsx ./**/*.js ./**/*.jsx",
    "lint:fix": "eslint ./**/*.ts ./**/*.tsx ./**/*.js ./**/*.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
